{
  "compilerOptions": {
    "target": "ES2021",
    "lib": [
      "es2017"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": [
    "./scripts/*"
  ]
}
